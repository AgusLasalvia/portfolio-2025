---
import "../styles/navbar.css";

const links = [
	{ label: "About", href: "#" },
	{ label: "Experiencia", href: "#experience" },
	{ label: "Projects", href: "#projects" },
];
---

<nav class="navbar">
	<ul class="nav-list">
		{
			links.map((link) => (
				<li>
					<a href={link.href} class="nav-link">
						{link.label}
						<span class="underline" />
					</a>
				</li>
			))
		}
	</ul>
</nav>
